FC          = @F77@
FFLAGS      = @PAPIPREP@ @FORTDEFS@ @FFLAGS@ @BITFLAGS@ @FTHREADFLAGS@
LDFLAGS     = @BITFLAGS@ @FTHREADFLAGS@ -L.. -lgptl @LDFLAGS@ @LIBS@

OBJS = errtest.o papiomptest.o stride.o camtest.o
EXES = errtest   papiomptest   stride   camtest

all: $(EXES)

clean:
	$(RM) $(OBJS) $(EXES)

errtest: errtest.o
	$(FC) $(FFLAGS) -o $@ errtest.o $(LDFLAGS)

papiomptest: papiomptest.o
	$(FC) $(FFLAGS) -o $@ papiomptest.o $(LDFLAGS)

stride: stride.o
	$(FC) $(FFLAGS) -o $@ stride.o $(LDFLAGS)

camtest: camtest.o
	$(FC) $(FFLAGS) -o $@ camtest.o $(LDFLAGS)
